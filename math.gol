func pow(int a, int b) int {
    if (b == 0) {
        return 1;
    } else {
        return a * pow(a, b - 1);
    }
}

func modulo(int a, int b) int {
    while (a >= b) {
        a = a - b;
    }
    return a;
}

func sqrt(int a) int {
    def int x = a;
    def int y = 1;
    while (x > y) {
        x = (x + y) / 2;
        y = a / x;
    }
    return x;
}

func ceil(float a) int {
    def int b = a;
    if (a == b) {
        return b;
    } else {
        return b + 1;
    }
}

func floor(float a) int {
    def int b = a;
    return b;
}

func iabs(int a) int {
    if (a < 0) {
        return -a;
    } else {
        return a;
    }
}

func fabs(float a) float {
    if (a < 0) {
        return -a;
    } else {
        return a;
    }
}

func fact(int a) int {
    if (a == 0) {
        return 1;
    } else {
        return a * fact(a - 1);
    }
}

